{
  "name": "react-extension-boilerplate",
  "version": "1.0.0",
  "description": "Modern React boilerplate for Firefox and Chrome extensions",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:chrome": "npm run chrome-launch",
    "start:firefox": "npm run firefox-launch",
    "start:views": "node scripts/start.js",
    "dev": "node scripts/dev.js",
    "build": "node scripts/build.js",
    "compress": "node scripts/compress",
    "compress-keygen": "crx keygen",
    "clean": "rimraf build/ dev/ dist/ extension/ *.zip *.crx",
    "lint": "eslint ./ ",
    "lint:fix": "eslint ./ --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chrome-launch": "node scripts/dev.js --browser=chrome",
    "firefox-launch": "node scripts/dev.js --browser=firefox"
  },
  "keywords": [
    "chrome",
    "extension",
    "react",
    "redux",
    "hot reload",
    "webpack",
    "boilerplate"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kryptokinght/react-extension-boilerplate.git"
  },
  "author": "Minanshu Singh <minanshu.1998@gmail.com>",
  "license": "MIT",
